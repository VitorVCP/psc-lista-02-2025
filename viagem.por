programa {
  funcao inicio() {
    real alemanha, portugal, italia,turista
    escreva ("Insira o valor das três viagens: ")
    leia (alemanha)
    leia (portugal)
    leia (italia)
    escreva ("Quantos turistass são? ")
    leia (turista)
    escreva ("Com ", (turista), " turistas, a viagem sairá por: ", (turista *(alemanha + portugal + italia)), ".")
  }
}
